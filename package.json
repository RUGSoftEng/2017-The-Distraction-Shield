{
  "name": "distraction-shield",
  "version": "2.2.0",
  "description": "Distraction Shield!",
  "scripts": {
    "start": "broccoli-timepiece dist/",
    "build": "rimraf dist && broccoli build dist",
    "build:production": "npm run build env=production && npm run pack",
    "build:test": "npm run build env=test",
    "test": "npm run build:test && ava dist/tests.js --verbose",
    "test:ci": "npm run build && ava dist/tests.js --verbose",
    "docs": "jsdoc ./\"Distraction Shield\" -c .jsdoc.json",
    "deploy": "node ./scripts/deploy-gh-pages",
    "pack": "crx pack dist/ -o dist/$npm_package_name-$npm_package_version.crx"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RUGSoftEng/2017-The-Distraction-Shield.git"
  },
  "license": "MIT",
  "devDependencies": {
    "async": "^2.4.1",
    "ava": "^0.19.1",
    "babel": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "broccoli": "^1.1.2",
    "broccoli-babel-transpiler": "^6.0.0",
    "broccoli-clean-css": "^2.0.1",
    "broccoli-cli": "^1.0.0",
    "broccoli-concat": "^3.2.2",
    "broccoli-funnel": "^1.2.0",
    "broccoli-merge-trees": "^2.0.0",
    "broccoli-rollup": "^1.3.0",
    "broccoli-timepiece": "^0.3.0",
    "chalk": "^1.1.3",
    "command-line-args": "^4.0.6",
    "console-ui": "^1.0.3",
    "crx": "^3.2.1",
    "esdoc": "^0.5.2",
    "eslint": "^4.0.0",
    "gh-pages": "^1.0.0",
    "git-rev-sync": "^1.9.1",
    "jsdoc": "^3.4.3",
    "mocha": "^3.3.0",
    "rewire": "^2.5.2",
    "rimraf": "^2.6.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-filesize": "^1.3.2",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "sinon": "^2.3.4",
    "sinon-chrome": "^2.2.1",
    "uuid": "^3.0.1"
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "js-uuid": "^0.0.6"
  }
}
